{
  "name": "configurator",
  "version": "1.0.0",
  "description": "Parcel configurator with Ink",
  "main": "dist/index.js",
  "repository": "git@github.com:CorentinAndre/configurator.git",
  "author": "Corentin Andr√© <corentin7andre8@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "parcel watch src/index.js --target node",
    "build": "parcel build src/index.js --target node --no-source-maps"
  },
  "precommit": "NODE_ENV=production lint-staged",
  "lint-staged": {
    "*.{js,jsx}": [
      "pretty-quick --staged",
      "eslint src/ --fix",
      "git add"
    ]
  },
  "dependencies": {
    "figures": "^2.0.0",
    "husky": "^1.3.1",
    "ink": "^2.0.5",
    "lint-staged": "^8.1.5",
    "pretty-quick": "^1.10.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.4"
  },
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/plugin-proposal-class-properties": "^7.3.4",
    "@babel/preset-react": "^7.0.0",
    "@ornikar/eslint-config-babel": "^8.0.1",
    "@ornikar/eslint-config-react": "^8.0.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.15.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prefer-class-properties": "^1.0.0",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-react-hooks": "^1.5.0",
    "eslint-plugin-unicorn": "^7.1.0",
    "parcel": "^1.12.0",
    "prettier": "^1.16.4"
  }
}
